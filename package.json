  "scripts": {
    "dev": "npm-run-all -p dev:electron dev:web",
    "dev:web": "ng serve --host 0.0.0.0 --disable-host-check -c dev -o",
    "dev:electron": "yarn electron:serve",
    "dev:server": "ng run nw-buddy:serve-ssr",
    "dev:wrangler": "wrangler pages dev dist/web-cloud",
    "build:web": "ng build --base-href / -c web",
    "build:server": "ng run nw-buddy:server:production",
    "build:web-cloud": "ng build --base-href / -c cloud && yarn nw-download -o dist/web-cloud/nw-data",
    "build:web-electron": "ng build --base-href ./ -c electron",
    "build:web-capacitor": "ng build --base-href ./ -c capacitor",
    "build:electron": "yarn electron:tsc && yarn build:web-electron && yarn electron:builder",
    "build:docker": "yarn build:web && yarn build:server && yarn docker:build",
    "storybook": "ng run nw-buddy:storybook",
    "storybook:build": "ng run nw-buddy:build-storybook",
    "electron:tsc": "tsc -p apps/electron/tsconfig.json",
    "electron:serve": "wait-on tcp:4200 && yarn electron:tsc && electron . --serve",
    "electron:builder": "electron-builder build --publish=never --config=electron-config.js",
    "nw-import": "ts-node ./tools/nw-import.ts",
    "nw-extract": "ts-node ./tools/nw-extract.ts",
    "nw-upload": "ts-node ./tools/nw-upload.ts",
    "nw-download": "ts-node ./tools/nw-download.ts",
    "cap:sync": "npx cap sync",
    "cap:android:open": "npx cap open android",
    "cap:android:run": "npx cap run android",
    "docker:build": "docker build -f Dockerfile -t nw-buddy:latest .",
    "docker:stop": "docker-compose -f ./docker-compose.yml -p nw-buddy stop",
    "docker:start": "docker-compose -f ./docker-compose.yml -p nw-buddy up -d",
    "docker:restart": "yarn docker:stop && yarn docker:start",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "lint": "ng lint"
  },
