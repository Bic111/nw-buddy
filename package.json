  "scripts": {
    "lint": "ng lint",
    "tauri": "tauri",
    "dev": "pnpm nw-generate env -c dev && npm-run-all -p dev:electron dev:web",
    "dev:electron": "pnpm electron:serve",
    "dev:web": "pnpm nw-generate env -c dev && ng serve --host 0.0.0.0 --disable-host-check -c dev -o",
    "dev:web-cdn": "pnpm nw-generate env -c dev-cdn && ng serve --host 0.0.0.0 --disable-host-check -c dev-cdn -o",
    "dev:ssr": "pnpm nw-generate env -c dev-ssr && ng serve --host 0.0.0.0 --disable-host-check -c dev-ssr -o",
    "dev:server": "ng run nw-buddy:serve-ssr",
    "dev:tools": "pnpm build:tools -w",
    "dev:storybook": "ng run nw-buddy:storybook",
    "dev:e2e": "pnpm playwright test --ui",
    "dev:wrangler": "npx wrangler pages dev dist/web/browser",
    "test:storybook": "concurrently -n SERVER,STORY -c red,green -k -s first \"pnpm storybook:serve --silent\" \"pnpm storybook:wait && pnpm storybook:test\"",
    "test:storybook:coverage": "concurrently -n SERVER,STORY -c red,green -k -s first \"pnpm storybook:serve --silent\" \"pnpm storybook:wait && pnpm storybook:test --coverage\"",
    "test:unit": "ng test --watch=false",
    "test:unit:watch": "ng test",
    "test:e2e": "pnpm playwright test",
    "build:cloudflare": "pnpm nw-cdn download && pnpm build:web",
    "build:electron": "pnpm electron:tsc && pnpm build:web-standalone && pnpm electron:builder",
    "build:tools": "nx run @nw-buddy/tools:build",
    "build:storybook": "pnpm storybook:build",
    "build:docker:ssr": "pnpm build:web-ssr && pnpm docker:build:ssr",
    "build:docker:ssr-cdn": "pnpm build:web-ssr-cdn && pnpm docker:build:ssr",
    "build:docker:web": "pnpm build:web && pnpm docker:build",
    "build:docker:web-cdn": "pnpm build:web-cdn && pnpm docker:build",
    "build:web": "pnpm nw-generate env -c web && ng build -c web --ssr false && pnpm nw-generate version",
    "build:web-cdn": "pnpm nw-generate env -c web-cdn && ng build -c web-cdn --ssr false && pnpm nw-generate version",
    "build:web-ssr": "pnpm nw-generate env -c web && ng build -c web && pnpm nw-generate version",
    "build:web-ssr-cdn": "pnpm nw-generate env -c web-cdn && ng build -c web-cdn && pnpm nw-generate version",
    "build:web-standalone": "pnpm nw-generate env -c standalone && ng build -c standalone",
    "electron:tsc": "tsc -p apps/electron/tsconfig.json",
    "electron:serve": "pnpm electron:tsc && electron . --serve",
    "electron:builder": "electron-builder build --publish=never --config=electron-config.js",
    "nw-generate": "ts-node ./tools/nw-generate.ts",
    "nw-inspect": "ts-node ./tools/nw-inspect.ts",
    "nw-cdn": "ts-node ./tools/nw-cdn.ts",
    "nw-functions": "ts-node ./tools/nw-functions.ts",
    "nw-buddy": "pnpm build:tools && env-cmd --silent  -f .env node ./tools/dist/tools/nw-buddy.js",
    "nw-data-update": "pnpm nw-buddy unpack && pnpm nw-buddy convert && pnpm nw-buddy import",
    "nw-quick-update": "pnpm nw-buddy extract -f **/*.datasheet **/*.loc.xml **/*.json && pnpm nw-buddy import \"locale,tables,code\"",
    "cap:sync": "npx cap sync",
    "cap:android:open": "npx cap open android",
    "cap:android:run": "npx cap run android",
    "docker:build": "docker build -f apps/docker/Dockerfile.static -t nw-buddy:latest .",
    "docker:build:ssr": "docker build -f apps/docker/Dockerfile.ssr -t nw-buddy:latest .",
    "docker:stop": "docker-compose -f apps/docker/compose.yml -p nw-buddy stop",
    "docker:start": "docker-compose -f apps/docker/compose.yml -p nw-buddy up -d",
    "docker:restart": "pnpm docker:stop && pnpm docker:start",
    "release": "pnpm nw-cdn upload && git push",
    "storybook:dev": "ng run nw-buddy:storybook",
    "storybook:build": "ng run nw-buddy:build-storybook",
    "storybook:serve": "npx http-server dist/web-storybook --port 5200",
    "storybook:wait": "pnpm wait-on http://127.0.0.1:5200",
    "storybook:test": "pnpm test-storybook -c apps/storybook --url http://127.0.0.1:5200",
    "chromatic": "npx chromatic --storybook-build-dir dist/web-storybook",
    "nwbt": "go run ./tools/nw-kit",
    "nwbt:test": "go test ./tools/nw-kit/...",
    "nwbt:generate": "go generate ./tools/nw-kit/..."
  },
