  "scripts": {
    "lint": "ng lint",
    "dev": "env-cmd -f .env npm-run-all -p dev:electron dev:web",
    "dev:web": "env-cmd -f .env ng serve --host 0.0.0.0 --disable-host-check -c dev -o",
    "dev:electron": "yarn electron:serve",
    "dev:server": "ng run nw-buddy:serve-ssr",
    "dev:wrangler": "wrangler pages dev dist/web-cloud",
    "dev:tools": "yarn build:tools -w",
    "dev:storybook": "ng run nw-buddy:storybook",
    "test:storybook": "concurrently -n SERVER,STORY -c red,green -k -s first \"yarn storybook:serve --silent\" \"yarn storybook:wait && yarn storybook:test\"",
    "test:storybook:coverage": "concurrently -n SERVER,STORY -c red,green -k -s first \"yarn storybook:serve --silent\" \"yarn storybook:wait && yarn storybook:test --coverage\"",
    "test:unit": "ng test --watch=false",
    "test:unit:watch": "ng test",
    "build:cloudflare": "yarn nw-cdn download && yarn build:web",
    "build:electron": "yarn electron:tsc && yarn build:web-electron && yarn electron:builder",
    "build:docker": "yarn build:web && yarn build:server && yarn docker:build",
    "build:tools": "tsc -p ./tools/tsconfig.json",
    "build:server": "ng run nw-buddy:server:production",
    "build:storybook": "yarn storybook:build",
    "build:web": "ng build --base-href / -c web",
    "build:web-electron": "ng build --base-href ./ -c electron",
    "build:web-capacitor": "ng build --base-href ./ -c capacitor",
    "build:web-overwolf": "ng build -c overwolf",
    "electron:tsc": "tsc -p apps/electron/tsconfig.json",
    "electron:serve": "yarn electron:tsc && electron . --serve",
    "electron:builder": "electron-builder build --publish=never --config=electron-config.js",
    "nw-cdn": "ts-node ./tools/nw-cdn.ts",
    "nw-extract": "yarn build:tools && env-cmd -f .env node ./dist/tools/nw-extract.js",
    "nw-convert": "yarn build:tools && env-cmd -f .env node ./dist/tools/nw-convert.js",
    "nw-import": "yarn build:tools && env-cmd -f .env node ./dist/tools/nw-import.js",
    "nw-codegen": "yarn build:tools && env-cmd -f .env node ./dist/tools/nw-codegen.js",
    "nw-data-update": "yarn nw-extract && yarn nw-convert -u && yarn nw-import -u",
    "cap:sync": "npx cap sync",
    "cap:android:open": "npx cap open android",
    "cap:android:run": "npx cap run android",
    "docker:build": "docker build -f Dockerfile -t nw-buddy:latest .",
    "docker:stop": "docker-compose -f ./docker-compose.yml -p nw-buddy stop",
    "docker:start": "docker-compose -f ./docker-compose.yml -p nw-buddy up -d",
    "docker:restart": "yarn docker:stop && yarn docker:start",
    "e2e:test": "concurrently -n NG,CY -c red,green,blue -k -s first 'yarn dev:web' 'wait-on tcp:4200 && cypress run' ",
    "e2e:dev": " concurrently -n NG,CY -c red,green,blue -k -s first 'yarn dev:web' 'wait-on tcp:4200 && cypress open'",
    "cy:open": "cypress open",
    "release": "yarn nw-cdn upload && git push",
    "storybook:dev": "ng run nw-buddy:storybook",
    "storybook:build": "ng run nw-buddy:build-storybook",
    "storybook:serve": "npx http-server dist/web-storybook --port 5200",
    "storybook:wait": "yarn wait-on http://127.0.0.1:5200",
    "storybook:test": "yarn test-storybook -c apps/storybook --url http://127.0.0.1:5200",
    "chromatic": "npx chromatic --storybook-build-dir dist/web-storybook"
  },
