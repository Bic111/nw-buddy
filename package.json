{
  "name": "nw-buddy",
  "version": "1.7.0-8",
  "description": "New World Buddy",
  "keywords": [],
  "main": "dist/electron/main.js",
  "private": true,
  "author": "Ginie",
  "homepage": "https://giniedp.github.io/nw-buddy",
  "repository": {
    "type": "git",
    "url": "https://github.com/giniedp/nw-buddy.git"
  },
  "scripts": {
    "dev": "npm-run-all -p dev:electron dev:web",
    "dev:web": "ng serve --host 0.0.0.0 --disable-host-check -c dev -o",
    "dev:electron": "yarn electron:serve",
    "build": "yarn build:electron && yarn build:web && electron-builder build --publish=never --config=electron-config.js",
    "build:web": "ng build --base-href ./",
    "build:web:electron": "yarn build:web -c electron",
    "build:web:capacitor": "yarn build:web -c capacitor",
    "build:electron": "yarn electron:tsc",
    "ssr:dev": "ng run nw-buddy:serve-ssr",
    "ssr:serve": "node dist/server/main.js",
    "ssr:build": "ng build --base-href / && ng run nw-buddy:server",
    "storybook": "ng run nw-buddy:storybook",
    "storybook:build": "ng run nw-buddy:build-storybook",
    "electron:tsc": "tsc -p apps/electron/tsconfig.json",
    "electron:serve": "wait-on tcp:4200 && yarn electron:tsc && electron . --serve",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "lint": "ng lint",
    "nw-import": "ts-node ./tools/nw-import.ts",
    "nw-extract": "ts-node ./tools/nw-extract.ts",
    "cap:sync": "npx cap sync",
    "cap:android:open": "npx cap open android",
    "cap:android:run": "npx cap run android"
  },
