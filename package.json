{
  "name": "nw-buddy",
  "version": "1.7.0-2",
  "description": "New World Buddy",
  "keywords": [],
  "main": "dist/electron/main.js",
  "private": true,
  "author": "Ginie",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "dev": "npm-run-all -p dev:electron dev:web",
    "dev:electron": "npm run electron:serve",
    "dev:web": "npm run web:serve",
    "build": "npm run electron:tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "storybook": "ng run nw-buddy:storybook",
    "storybook:build": "ng run nw-buddy:build-storybook",
    "web:serve": "ng serve -c web -o",
    "electron": "electron",
    "electron:tsc": "tsc -p apps/electron/tsconfig.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "lint": "ng lint",
    "import": "ts-node ./tools/nw-import.ts ./tmp/nw-data/live ./apps/web/nw-data",
    "import:ptr": "ts-node ./tools/nw-import.ts ./tmp/nw-data/ptr ./apps/web/nw-data",
    "extract": "yarn nw-extract \"C:\\Program Files (x86)\\Steam\\steamapps\\common\\New World\" -o ./tmp/nw-data/live -f \"datasheet:json,locale:json,icon:png,image:png\"",
    "extract:ptr": "yarn nw-extract \"G:\\Steam\\steamapps\\common\\New World Public Test Realm\" -o ./tmp/nw-data/ptr -f \"datasheet:json,locale:json,icon:png,image:png\""
  },
